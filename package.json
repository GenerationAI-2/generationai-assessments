{
  "name": "generation-ai-tools",
  "version": "1.0.0",
  "private": true,
  "description": "GenerationAI Tools Monorepo - AI Risk Assessment and Compliance Tools",
  "scripts": {
    "build": "pnpm -r build",
    "build:shadow-ai": "pnpm --filter shadow-ai-assessment-api build",
    "build:business-readiness": "pnpm --filter business-readiness-assessment-api build",
    "build:board-governance": "pnpm --filter board-governance-assessment-api build",
    "build:personal-ai-readiness": "pnpm --filter personal-ai-readiness-api build",
    "build:pdf": "pnpm --filter pdf-generator build",
    "dev:shadow-ai": "pnpm --filter shadow-ai-assessment-api dev",
    "dev:business-readiness": "pnpm --filter business-readiness-assessment-api dev",
    "dev:board-governance": "pnpm --filter board-governance-assessment-api dev",
    "dev:personal-ai-readiness": "pnpm --filter personal-ai-readiness-api dev",
    "dev:pdf": "pnpm --filter pdf-generator dev",
    "start:shadow-ai": "pnpm --filter shadow-ai-assessment-api start",
    "start:business-readiness": "pnpm --filter business-readiness-assessment-api start",
    "start:board-governance": "pnpm --filter board-governance-assessment-api start",
    "start:personal-ai-readiness": "pnpm --filter personal-ai-readiness-api start",
    "start:pdf": "pnpm --filter pdf-generator start",
    "clean": "pnpm -r clean",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test"
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  }
}